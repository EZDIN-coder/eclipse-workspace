	    	   double temps; 
	    
			System.out.println("Vous roulez a "+vitesse+" et pour une distance de " + distance+" Vous mettez un temps de "+temps);
	    
	    }
	 public double temps(double vitesse) {
			
			double temps = 100/vitesse;
			
			return temps;
	 }
